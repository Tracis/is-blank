  "main": "lib/index.js",
  "module": "lib/index.js",
  "typings": "./index.d.ts",
  "scripts": {
    "test": "jest",
    "clean": "rimraf lib dist es",
    "build": "npm run clean && npm run build:commonjs && npm run build:es",
